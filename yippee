# first draft

data = {'Species': ['Bear', 'Cat', 'Dog', 'Elephant', 'Goat', 'Horse', 'Lion', 'Tiger', 'Wolf'], 'Number of images': [97, 99, 97, 97, 85, 67, 95 ,96, 97]}

